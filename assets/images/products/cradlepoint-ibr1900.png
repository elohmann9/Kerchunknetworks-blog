  <!-- Dynamically replace phone elements with phone number -->
  <script>
    function replacePhoneIcon() {
      // Try multiple selectors to find the phone element
      const selectors = [
        '.header-icons-wrapper_ph',
        'a.header-icons-wrapper_ico',
        '.header-icon.phone-call',
        '[class*="phone"]',
        'a[href^="tel:"]'
      ];

      for (let selector of selectors) {
        const element = document.querySelector(selector);
        const phone = document.querySelector('.icon.icon-phone-classic');
        const phoneHover = document.querySelector('.header-icons-wrapper__phone');

        if (element) {
          phone.classList.remove('icon-phone-classic');

          element.innerHTML = '1-855-813-3385';
          element.style.color = '#fff';
          element.style.fontFamily = 'inherit !important';

          if (element.tagName === 'A') {
            element.href = 'tel:1-855-813-3385';
          }
          return;
        }
      }

      // If not found, try again after a short delay
      setTimeout(replacePhoneIcon, 100);
    }

    // Run when DOM is ready
    if (document.readyState === 'loading') {
      document.addEventListener('DOMContentLoaded', replacePhoneIcon);
    } else {
      replacePhoneIcon();
    }

    // Also run after a delay to catch dynamically loaded content
    setTimeout(replacePhoneIcon, 500);
  </script>

