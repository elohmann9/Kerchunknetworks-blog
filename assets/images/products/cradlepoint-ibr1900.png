  <script>
    function tweakPhoneIcon() {
      var host = document.querySelector('cradlepoint-nav-main');
      var roots = [];
      if (host && host.shadowRoot) roots.push(host.shadowRoot);
      roots.push(document);

      var target = null;
      for (var i = 0; i < roots.length; i++) {
        var root = roots[i];
        var el = root.querySelector('a.header-icons-wrapper__icon.header-icons-wrapper__phone');
        if (el) {
          target = el;
          // Inject CSS to hide :before/:after
          var styleId = 'hide-phone-pseudo-style';
          if (!(root.getElementById && root.getElementById(styleId))) {
            var style = document.createElement('style');
            style.id = styleId;
            style.textContent = 'a.header-icons-wrapper__icon.header-icons-wrapper__phone:before, a.header-icons-wrapper__icon.header-icons-wrapper__phone:after { display: none !important; content: none !important; }';
            (root.head || root).appendChild(style);
          }
          break;
        }
      }

      if (!target) {
        setTimeout(tweakPhoneIcon, 100);
      }
    }

    if (window.customElements && customElements.whenDefined) {
