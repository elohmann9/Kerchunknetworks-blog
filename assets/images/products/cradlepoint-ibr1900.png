<script async defer>
((w) => {
  let startButtonInMainNav = null;
  if (!startButtonInMainNav) {
    let startButtonInMainNavInterval = setInterval(() => {
      startButtonInMainNav = document.querySelector('cradlepoint-nav-main').shadowRoot.querySelector('a.how_to_buy_chat_with_us');
      if (startButtonInMainNav) {
        clearInterval(startButtonInMainNavInterval);
        console.log(startButtonInMainNav);
        startButtonInMainNav.addEventListener('click', (e) => {
          e.preventDefault();
          console.log(w.drift);
          if ( window.drift || window.driftt ) {
            let drift = window.drift || window.driftt;
            drift.api.startInteraction({ interactionId:parseInt('159129') });
          }
        });
      }
    }, 500)
  }
  let mainNav = null;
  if (!mainNav) {
  	let startMainNavInterval = setInterval(() => {
    	mainNav = document.querySelector('cradlepoint-nav-main');
      	console.log('MAINNAV', mainNav);
      if (mainNav) {
      	clearInterval(startMainNavInterval);
        let jsonURL = mainNav.attributes[0].value;
        console.log('JSONURL', jsonURL);
        mainNav.attributes[0].value = '/wp-content/uploads/cradlepoint-posts-json/post-15867.json';
      }
    }, 500)
  }
})(window)
</script>
